                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>SELECT scheme.sir_rldraft.rl_period, substring(datename(mm,rlclm.from_date),1,3)+' '+rtrim(convert(char(2),day(rlclm.from_date)))+' to '+
substring(datename(mm,rlclm.to_date),1,3)+' '+ltrim(convert(char(2),day(rlclm.to_date))) as tsperiod,
scheme.sir_rldraft.resource_code, scheme.sir_rldraft.description, 
and_ts_draft.rl_status, scheme.sir_rldraft.timesheet_ref, scheme.sir_rldraft.ugroup, scheme.sir_rldraft.cost_centre, substring(datename(mm,scheme.sir_rldraft.entry_date),1,3)+' '+rtrim(convert(char(2),day(scheme.sir_rldraft.entry_date))) as entry_date,sir_rlnotes.notes
FROM sir_rldraft INNER JOIN and_ts_draft ON scheme.sir_rldraft.resource_code = and_ts_draft.resource_code AND 
scheme.sir_rldraft.rl_period = and_ts_draft.rl_period INNER JOIN rlclm ON scheme.sir_rldraft.rl_period = rlclm.period 
LEFT JOIN sir_rlnotes ON scheme.sir_rldraft.timesheet_ref = sir_rlnotes.timesheet_ref 
WHERE (scheme.sir_rldraft.resource_code=@WhatEmpNo)</CommandText>
